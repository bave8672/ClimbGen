<?xml version='1.0' encoding='utf-8'?>
<widget android-packageName="com.company.androidapp" id="net.baves.climbgen" ios-CFBundleIdentifier="295AQHVL3J.net.baves.climbgen" version="1.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="AdMob">
        <param name="android-package" value="com.rjfun.cordova.admob.AdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <icon density="ldpi" src="www/icon/android/drawable-ldpi/ic_launcher.png" />
    <icon density="mdpi" src="www/icon/android/drawable-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="www/icon/android/drawable-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="www/icon/android/drawable-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="www/icon/android/drawable-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="www/icon/android/drawable-xxxhdpi/ic_launcher.png" />
    <name>ClimbGen</name>
    <description>
        A workout generator and randomiser for rock climbing.
    </description>
    <author email="avesbenjamin@gmail.com" href="baves.net">
        Ben Aves
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="deployment-target" value="7.0" />
</widget>
